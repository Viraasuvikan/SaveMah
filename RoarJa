return x.body
end

function cekbp()
if not BpCek then
if getBot():getLocal().slot < 36 then
if getBot():getLocal().gems > 300 then
repeat
getBot():sendPacket("action|buy\nitem|upgrade_backpack",2)
sleep(3000)
until getBot():getLocal().slot == 36
BpCek = true
end
end
end
end

